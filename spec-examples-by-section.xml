<?xml version="1.0" encoding="UTF-8"?>
<tests>
   <test name="3.2.1 Variable Expansion">
      <variables>
         <variable name="count">
            <array>
               <string>one</string>
               <string>two</string>
               <string>three</string>
            </array>
         </variable>
         <variable name="dom">
            <array>
               <string>example</string>
               <string>com</string>
            </array>
         </variable>
         <variable name="dub">
            <string>me/too</string>
         </variable>
         <variable name="hello">
            <string>Hello World!</string>
         </variable>
         <variable name="half">
            <string>50%</string>
         </variable>
         <variable name="var">
            <string>value</string>
         </variable>
         <variable name="who">
            <string>fred</string>
         </variable>
         <variable name="base">
            <string>http://example.com/home/</string>
         </variable>
         <variable name="path">
            <string>/foo/bar</string>
         </variable>
         <variable name="list">
            <array>
               <string>red</string>
               <string>green</string>
               <string>blue</string>
            </array>
         </variable>
         <variable name="keys">
            <object>
               <field name="semi">
                  <string>;</string>
               </field>
               <field name="dot">
                  <string>.</string>
               </field>
               <field name="comma">
                  <string>,</string>
               </field>
            </object>
         </variable>
         <variable name="v">
            <string>6</string>
         </variable>
         <variable name="x">
            <string>1024</string>
         </variable>
         <variable name="y">
            <string>768</string>
         </variable>
         <variable name="empty">
            <string/>
         </variable>
         <variable name="empty_keys">
            <array/>
         </variable>
         <variable name="undef">
            <null/>
         </variable>
      </variables>
      <testcases>
         <testcase template="{count}" result="one,two,three"/>
         <testcase template="{count*}" result="one,two,three"/>
         <testcase template="{/count}" result="/one,two,three"/>
         <testcase template="{/count*}" result="/one/two/three"/>
         <testcase template="{;count}" result=";count=one,two,three"/>
         <testcase template="{;count*}" result=";count=one;count=two;count=three"/>
         <testcase template="{?count}" result="?count=one,two,three"/>
         <testcase template="{?count*}" result="?count=one&amp;count=two&amp;count=three"/>
         <testcase template="{&amp;count*}" result="&amp;count=one&amp;count=two&amp;count=three"/>
      </testcases>
   </test>
